{
    "name": "simoneddy/afltables-crawler",
    "description": "Crawls afltables.com",
    "type": "library",
    "license": "MIT",
    "config":{
        "bin-dir": "bin"
    },
    "authors": [
        {
            "name": "Simon Eddy",
            "email": "simon@simoneddy.com.au"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "AflCrawler\\": "src"
        }
    },
    "require": {
        "doctrine/orm": "^2.6",
        "guzzlehttp/guzzle": "^6.3",
        "nesbot/carbon": "^2.14",
        "symfony/browser-kit": "^4.2",
        "symfony/console": "^4.2",
        "symfony/css-selector": "^4.2",
        "symfony/dom-crawler": "^4.2",
        "symfony/finder": "^4.3",
        "symfony/options-resolver": "^4.2",
        "symfony/yaml": "^4.2",
        "symfony/filesystem": "^4.3",
        "pimple/pimple": "^3.2",
        "league/pipeline": "^1.0",
        "adbario/php-dot-notation": "^2.2"
    },
    "require-dev": {
        "symfony/var-dumper": "^4.3",
        "phpspec/phpspec": "^6.0",
        "behat/behat": "^3.5"
    }
}
